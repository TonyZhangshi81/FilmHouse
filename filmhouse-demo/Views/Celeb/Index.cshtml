@{
    ViewData["Title"] = @ViewBag.Name;
}

@{
    Layout = "~/Views/Shared/_LayoutWide.cshtml";
}

@section Links {
    <link rel="stylesheet" href="~/css/celeb.css" />
}

@section Scripts {
    <script src="~/lib/smoothscroll/jquery.smooth-scroll.js"></script>
    <script src="~/lib/backstrech/jquery.backstrech.min.js"></script>
    <script src="~/js/Celeb/Index.js"></script>
}

@{
    var html = $"Celeb{ViewBag.CelebId}.Index.Content.html";
    <include-content name=@html />
}
